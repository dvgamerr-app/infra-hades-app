{
  "name": "infar-app",
  "version": "1.0.0",
  "main": "dist-electron/main/index.js",
  "description": "Really simple Electron + Vue + Vite boilerplate.",
  "author": "Kananek T. <info.dvgamer@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
