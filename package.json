{
  "name": "infra-hades",
  "version": "0.1.0",
  "main": "dist-electron/main/index.js",
  "description": "Instantly manage and monitor Kubernetes from your Desktop.",
  "author": "Kananek T. <info.dvgamer@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "wasm": "wasm-pack build .\\hades --target web",
    "dev": "vite",
    "build:vite": "vite build && mv ./dist/assets/*.svg ./dist/ >nul",
    "build": "yarn wasm && vue-tsc --noEmit && yarn build:vite && electron-builder",
    "prepare": "husky install"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/vue-fontawesome": "^3.0.0-4",
    "@types/node": "18.8.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/composition-api": "^1.7.1",
    "@vue/eslint-config-typescript": "^11.0.2",
    "bootstrap": "^5.2.2",
    "electron": "^21.2.0",
    "electron-builder": "^23.3.3",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.6.0",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "sass": "^1.42.1",
    "sass-loader": "^10.2.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.4",
    "vite-electron-plugin": "^0.4.6",
    "vite-plugin-electron-renderer": "^0.10.1",
    "vite-plugin-fonts": "^0.6.0",
    "vite-plugin-wasm-pack": "^0.1.12",
    "vue": "^3.2.41",
    "vue-tsc": "^1.0.8"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344"
    }
  },
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "dependencies": {
    "electron-settings": "^4.0.2",
    "yaml": "^2.1.3"
  }
}
